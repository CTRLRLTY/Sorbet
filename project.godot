[application]

config/name="Sorbet"
run/main_scene="res://Scenes/TitleScreen/TitleScreen.tscn"
boot_splash/image="res://Resources/SplashScreen.png"
config/icon="res://icon.png"

[autoload]

Nakama="*res://addons/com.heroiclabs.nakama/Nakama.gd"
RuntimeManager="*res://Autoloads/RuntimeManager.gd"
SceneManager="*res://Autoloads/SceneManager.tscn"
Server="*res://Autoloads/Server.gd"

[debug]

gdscript/warnings/unused_variable=false
gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false

[display]

window/size/width=360
window/size/height=800
window/size/always_on_top=true
window/vsync/vsync_via_compositor=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

misc/lossless_compression/force_png=true
environment/default_environment="res://default_env.tres"
